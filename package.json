{
  "name": "egangotri-react-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@fontsource/roboto": "latest",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@mui/icons-material": "latest",
    "@mui/lab": "6.0.0-beta.26",
    "@mui/material": "^5.16.7",
    "@mui/x-data-grid": "6.5.0",
    "@mui/x-date-pickers": "^6.5.0",
    "@mui/x-date-pickers-pro": "^6.5.0",
    "@react-oauth/google": "^0.12.1",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.8",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-hook-form": "^7.46.2",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.27.0",
    "react-scripts": "4.0.3",
    "recoil": "^0.7.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/lodash": "latest",
    "@types/node": "22.7.9",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prettier": "^3.2.5",
    "serve": "^14.2.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "start0": "react-dotenv && pnpm run lint_fix",
    "startCRA": "pnpm watch:css && set NODE_OPTIONS=--openssl-legacy-provider && react-dotenv && pnpm run lint_fix && react-scripts start",
    "start2": "pnpm watch:css && react-scripts start",
    "buildCRA": "pnpm build:css &&  set NODE_OPTIONS=--openssl-legacy-provider && react-dotenv && react-scripts build",
    "build2": "pnpm build:css && set NODE_OPTIONS=--openssl-legacy-provider && react-dotenv && react-scripts build",
    "test": "react-dotenv && react-scripts test",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint_fix": "npx eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src/"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-dotenv": {
    "whitelist": [
      "REACT_APP_DEV_ENV"
    ]
  },
  "engines": {
    "node": "20.12.0"
  }
}