{
  "name": "egangotri-react-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@fontsource/roboto": "latest",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@mui/icons-material": "latest",
    "@mui/material": "^5.13.3",
    "@mui/x-data-grid": "6.5.0",
    "@mui/x-date-pickers": "^6.5.0",
    "@mui/x-date-pickers-pro": "^6.5.0",
    "@react-oauth/google": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.8",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-hook-form": "^7.46.2",
    "react-icons": "^4.9.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "4.0.3",
    "recoil": "^0.7.7",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "web-vitals": "^1.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/user-event": "latest",
    "@types/jest": "^27.0.1",
    "@types/lodash": "latest",
    "@types/node": "18.17.x",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-pdf": "latest",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "serve": "^13.0.2"
  },
  "scripts": {
    "start0": "react-dotenv && yarn run lint_fix",
    "start": "yarn watch:css && set NODE_OPTIONS=--openssl-legacy-provider && react-dotenv && yarn run lint_fix && react-scripts start",
    "start2": "yarn watch:css && react-scripts start",
    "build": "yarn build:css && react-dotenv && react-scripts build",
    "build2": "yarn build:css && set NODE_OPTIONS=--openssl-legacy-provider && react-dotenv && react-scripts build",
    "test": "react-dotenv && react-scripts test",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint_fix": "npx eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src/"
          ]
        }
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-dotenv": {
    "whitelist": [
      "REACT_APP_DEV_ENV"
    ]
  },
  "engines": {
    "node": "18.17.x"
  }
}
